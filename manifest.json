{
    "manifest_version": 3,
    "name": "Earth Engine macOS Fix",
    "version": "1.0",
    "description": "Fixes keyboard accentuation and shortcut issues on Google Earth Engine for macOS.",
    "content_scripts": [
        {
            "matches": ["https://code.earthengine.google.com/*"],
            "js": ["content-scripts.js"],
            "run_at": "document_idle"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["page-script.js"],
            "matches": ["https://code.earthengine.google.com/*"]
        }
    ]
}