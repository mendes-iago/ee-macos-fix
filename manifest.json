{
    "manifest_version": 3,
    "name": "Earth Engine macOS Fix",
    "version": "1.0",
    "description": "Custom styles and keybindings for the Earth Engine code editor.",
    "content_scripts": [
        {
            "matches": [
                "https://code.earthengine.google.com/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_idle",
            "world": "MAIN"
        }
    ]
}