{
    "manifest_version": 3,
    "name": "EE macOS Fix",
    "version": "1.0",
    "description": "Fixes keybindings and applies custom styles to the EE Code Editor on macOS.",
    "content_scripts": [
        {
            "matches": [
                "https://code.earthengine.google.com/*"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "styles.css"
            ],
            "run_at": "document_idle",
            "world": "MAIN"
        }
    ]
}